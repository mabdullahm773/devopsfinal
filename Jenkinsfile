pipeline {
    agent any
    stages {
        stage('Setup') {
            steps {
                bat 'echo Setting up environment...'
                bat 'cd C:\\Path\\To\\Your\\Directory && your_command_here.bat'
            }
        }

        stage('Run Script') {
            steps {
                echo 'Running Hello World script...'
                // Run the Python script
               bat 'C:\\Users\\Mohammad Abdullah\\Desktop\\devopsfinal'
            }
        }
    }
}
